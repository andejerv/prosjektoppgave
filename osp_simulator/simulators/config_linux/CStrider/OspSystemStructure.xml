                <InitialValue variable="m"><Real value="3385.371"/></InitialValue>
                <InitialValue variable="m_added[1]"><Real value="259.13322"/></InitialValue>
                <InitialValue variable="m_added[2]"><Real value="2626.66858"/></InitialValue>
                <InitialValue variable="m_added[3]"><Real value="3398.91248"/></InitialValue>
