                <InitialValue variable="I_added[3]"><Real value="8500.86539"/></InitialValue>

            </InitialValues>
        </Simulator>
        <!-- Hydrodynamic model for the hull -->
        <Simulator name="hull_maneuvering" source="../../fmus_linux/hull_maneuvering.fmu">
			<InitialValues>
                <InitialValue variable="Xvv"><Real value="-0.04"/></InitialValue>
                <InitialValue variable="Xvr"><Real value="0.002"/></InitialValue>
                <InitialValue variable="Xrr"><Real value="0.011"/></InitialValue>
                <InitialValue variable="Xvvvv"><Real value="0.771"/></InitialValue>
                <InitialValue variable="Yv"><Real value="-0.315"/></InitialValue>
                <InitialValue variable="Yr"><Real value="0.083"/></InitialValue>
                <InitialValue variable="Yvvv"><Real value="-1.607"/></InitialValue>
                <InitialValue variable="Yvrr"><Real value="-0.391"/></InitialValue>
                <InitialValue variable="Yvvr"><Real value="0.379"/></InitialValue>
                <InitialValue variable="Yrrr"><Real value="0.008"/></InitialValue>
                <InitialValue variable="Nv"><Real value="-0.0315"/></InitialValue>
                <InitialValue variable="Nr"><Real value="-0.06351"/></InitialValue>
                <InitialValue variable="Nvvv"><Real value="-0.03"/></InitialValue>
                <InitialValue variable="Nvrr"><Real value="0.055"/></InitialValue>
                <InitialValue variable="Nvvr"><Real value="-0.294"/></InitialValue>
                <InitialValue variable="Nrrr"><Real value="-0.013"/></InitialValue>
                <InitialValue variable="shape_factor"><Real value="0.04771"/></InitialValue>
                <InitialValue variable="CR_m"><Real value="0.0168"/></InitialValue>
                <InitialValue variable="CR_p"><Real value="2.14441"/></InitialValue>
                <InitialValue variable="CD_lateral"><Real value="0.08"/></InitialValue>
                <InitialValue variable="length"><Real value="8.1"/></InitialValue>
                <InitialValue variable="depth"><Real value="0.35"/></InitialValue>
                <InitialValue variable="wetted_surface"><Real value="18.54978"/></InitialValue>




                <!-- Low speed model -->
                <InitialValue variable="CX_low"> <Real value="0.08"/></InitialValue>
                <InitialValue variable="CY_low"> <Real value="1.5"/></InitialValue>
                <InitialValue variable="CN_low"> <Real value="0.15"/></InitialValue>
                <InitialValue variable="Nr_low"> <Real value="-1000"/></InitialValue>

                <!-- Model mixing -->
                <InitialValue variable="speed_limit_low"> <Real value="0.5"/></InitialValue>
                <InitialValue variable="speed_transfer_range"> <Real value="0.25"/></InitialValue>
                <InitialValue variable="low_speed_model"><Boolean value="true"/></InitialValue>
                <InitialValue variable="high_speed_model"><Boolean value="true"/></InitialValue>

            </InitialValues>
		</Simulator>

        <!-- Seakeeping model for the hull -->
        <Simulator name="hull_seakeeping" source="../../fmus_linux/hull_seakeeping.fmu">
			<InitialValues>
                <InitialValue variable="mass"><Real value="3385.371"/></InitialValue>
                <InitialValue variable="waterplane_area"><Real value="18.468"/></InitialValue>
                <InitialValue variable="GM_roll"><Real value="3.0"/></InitialValue>
                <InitialValue variable="GM_pitch"><Real value="9.0"/></InitialValue>
                <InitialValue variable="heave_damping"><Real value="21301.91131"/></InitialValue>
                <InitialValue variable="roll_damping"><Real value="6095.77467"/></InitialValue>
                <InitialValue variable="pitch_damping"><Real value="49244.30873"/></InitialValue>

            </InitialValues>
		</Simulator>

        <Simulator name="Thruster STB" source="../../fmus_linux/azimuth_thruster.fmu">
			<InitialValues>
                <InitialValue variable="rudder_area"> <Real value="0.1"/></InitialValue>
                <InitialValue variable="rudder_chord"><Real value="0.2"/></InitialValue>

                <InitialValue variable="longitudinal_position"><Real value="-4.0"/></InitialValue>
                <InitialValue variable="lateral_position"><Real value="1.075"/></InitialValue>

                <InitialValue variable="ship_length"><Real value="8.1"/></InitialValue>

                <InitialValue variable="cd_k"> <Real value="19.92625501"/></InitialValue>
                <InitialValue variable="cd_a2"><Real value="0.0"/></InitialValue>
                
                <InitialValue variable="effective_aspect_ratio_lift"><Real value="0.8"/></InitialValue>
                <InitialValue variable="effective_aspect_ratio_drag"><Real value="0.55"/></InitialValue>

                <InitialValue variable="tr"><Real value="0.0"/></InitialValue>
                <InitialValue variable="ah"><Real value="0.22"/></InitialValue>
                <InitialValue variable="xh"><Real value="-0.055"/></InitialValue>

                <InitialValue variable="cd_stall_max"><Real value="1.0"/></InitialValue>
                <InitialValue variable="cd_stall_power"><Real value="1.6"/></InitialValue>

                <InitialValue variable="cl_stall_mean"><Real value="0.6"/></InitialValue>
                <InitialValue variable="cl_post_stall_max"><Real value="1.0"/></InitialValue>

                <InitialValue variable="propeller_geometry"><String value="WAGENINGEN_B4_BAR70_PD08"/></InitialValue>
                <InitialValue variable="pitch_scaling_factor"><Real value="1.0"/></InitialValue>
                <InitialValue variable="propeller_diameter"><Real value="0.28702"/></InitialValue>
				<InitialValue variable="wake_factor"><Real value="0.0"/></InitialValue>
                <InitialValue variable="thrust_deduction_factor"><Real value="0.0"/></InitialValue>

                <InitialValue variable="rotations_per_second"><Real value="0.0"/></InitialValue>
                <InitialValue variable="angle"><Real value="0.0"/></InitialValue>
            </InitialValues>
		</Simulator>

         <Simulator name="Steering machinery STB" source="../../fmus_linux/steering_machinery.fmu">
            <InitialValues>
                <InitialValue variable="max_speed">
                    <Real value="0.2"/><!--rad/s-->
                </InitialValue>
                <InitialValue variable="initial_angle">
                    <Real value="0.0"/>
                </InitialValue>
            </InitialValues>
        </Simulator>

        <Simulator name="Electric drive STB" source="../../fmus_linux/electric_drive.fmu">
            <InitialValues>
                <InitialValue variable="max_rpm">
                    <Real value="1700"/>
                </InitialValue>
                <InitialValue variable="time_constant">
                    <Real value="0.5"/>
                </InitialValue>
                <InitialValue variable="max_rpm_rate">
                    <Real value="1000"/>
                </InitialValue>
            </InitialValues>
        </Simulator>

        <Simulator name="propulsion_interface" source="../../fmus_linux/cstrider_propulsion_interface.fmu">
            <InitialValues>
                <InitialValue variable="modbus_server_ip">
                    <String value="133.13.0.3" />
                </InitialValue>
                <InitialValue variable="modbus_server_port">
                    <Integer value="10502" />
                </InitialValue>
                <InitialValue variable="holding_register_base_address">
                    <Integer value="0" />
                </InitialValue>
                <InitialValue variable="num_holding_registers">
                    <Integer value="252" />
                </InitialValue>
                <InitialValue variable="zeabuz_system_heartbeat_signal_address">
                    <Integer value="1" />
                </InitialValue>
                <InitialValue variable="zeabuz_system_available_signal_address">
                    <Integer value="2" />
                </InitialValue>
                <InitialValue variable="stb_propulsion_command_signal_address">
