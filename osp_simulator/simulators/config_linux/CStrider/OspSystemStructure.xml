                </InitialValue>
                <InitialValue variable="stb_steering_command_signal_address">
                    <Integer value="4" />
                </InitialValue>
                <InitialValue variable="port_propulsion_command_signal_address">
                    <Integer value="5" />
                </InitialValue>
