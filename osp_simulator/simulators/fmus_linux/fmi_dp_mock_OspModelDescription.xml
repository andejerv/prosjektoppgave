<?xml version="1.0" encoding="utf-8" ?>
<OspModelDescription xmlns="https://open-simulation-platform.com/OspModelDescription/1.0.0" version="1.0">
        
    <UnitDefinitions>
        <Unit name="m">
            <BaseUnit kg="1" m="1" s="0" A="0" K="0" mol="0" cd="0" rad="0" factor="1.0" offset="0.0"/>
        </Unit>
        <Unit name="rad">
            <BaseUnit kg="0" m="0" s="0" A="0" K="0" mol="0" cd="0" rad="1" factor="1.0" offset="0.0"/>
        </Unit>
        <Unit name="m/s">
            <BaseUnit kg="0" m="1" s="-1" A="0" K="0" mol="0" cd="0" rad="0" factor="1.0" offset="0.0"/>
        </Unit>
        <Unit name="rad/s">
            <BaseUnit kg="0" m="0" s="-1" A="0" K="0" mol="0" cd="0" rad="1" factor="1.0" offset="0.0"/>
        </Unit>
        <Unit name="N">
            <BaseUnit kg="1" m="1" s="-2" A="0" K="0" mol="0" cd="0" rad="0" factor="1.0" offset="0.0"/>
        </Unit>
        <Unit name="N.m">
            <BaseUnit kg="1" m="2" s="-2" A="0" K="0" mol="0" cd="0" rad="0" factor="1.0" offset="0.0"/>
        </Unit>
    </UnitDefinitions>

    <VariableGroups>
		
        <Generic name="position_measurements_3dof">
            <Variable ref="measurements.northEastHeading[0]" unit="m"/>
            <Variable ref="measurements.northEastHeading[1]" unit="m"/>
            <Variable ref="measurements.northEastHeading[2]" unit="rad"/>
        </Generic>

        <Generic name="velocity_measurements_3dof">
            <Variable ref="measurements.nu[0]" unit="m/s"/>
            <Variable ref="measurements.nu[1]" unit="m/s"/>
            <Variable ref="measurements.nu[2]" unit="rad/s"/>
        </Generic>

        <Generic name="generalizedForces_3dof">
            <Variable ref="generalizedForces[0]" unit="N"/>
            <Variable ref="generalizedForces[1]" unit="N"/>
            <Variable ref="generalizedForces[2]" unit="N.m"/>
        </Generic>

        <Generic name="autonomy_joystick_commands">
            <Variable ref="refForcesAutonomy[0]" unit="N"/>
            <Variable ref="refForcesAutonomy[1]" unit="N"/>
            <Variable ref="refForcesAutonomy[2]" unit="N.m"/>
        </Generic>

        <Generic name="autonomy_dp_commands">
            <Variable ref="refDpAutonomy.northEastHeading[0]" unit="m"/>
            <Variable ref="refDpAutonomy.northEastHeading[1]" unit="m"/>
            <Variable ref="refDpAutonomy.northEastHeading[2]" unit="rad"/>
            <Variable ref="refDpAutonomy.northEastHeadingVel[0]" unit="m/s"/>
            <Variable ref="refDpAutonomy.northEastHeadingVel[1]" unit="m/s"/>
            <Variable ref="refDpAutonomy.northEastHeadingVel[2]" unit="rad/s"/>
            <Variable ref="refDpAutonomy.northEastHeadingAccel[0]" unit="m/s^2"/>
            <Variable ref="refDpAutonomy.northEastHeadingAccel[1]" unit="m/s^2"/>
            <Variable ref="refDpAutonomy.northEastHeadingAccel[2]" unit="rad/s^2"/>
            <Variable ref="refDpAutonomy.surgeIntegratorMode"/>
            <Variable ref="refDpAutonomy.swayIntegratorMode"/>
            <Variable ref="refDpAutonomy.headingIntegratorMode"/>
        </Generic>

        <Generic name="autonomy_autopilot_commands">
            <Variable ref="autopilotInputsAutonomy.heading_ref" unit="rad"/>
            <Variable ref="autopilotInputsAutonomy.yaw_rate_ref" unit="rad/s"/>
            <Variable ref="autopilotInputsAutonomy.yaw_accel_ref" unit="rad/s/s"/>
            <Variable ref="autopilotInputsAutonomy.surge_speed_ref" unit="m/s"/>
            <Variable ref="autopilotInputsAutonomy.surge_accel_ref" unit="m/s/s"/>       
            <Variable ref="autopilotInputsAutonomy.surgeIntegratorMode"/>
            <Variable ref="autopilotInputsAutonomy.headingIntegratorMode"/>
        </Generic>

    </VariableGroups>
</OspModelDescription>
