<?xml version="1.0" encoding="utf-8" ?>
<OspModelDescription xmlns="https://open-simulation-platform.com/OspModelDescription/1.0.0" version="1.0">
        
    <UnitDefinitions>
        <Unit name="m">
            <BaseUnit kg="1" m="1" s="0" A="0" K="0" mol="0" cd="0" rad="0" factor="1.0" offset="0.0"/>
        </Unit>
        <Unit name="rad">
            <BaseUnit kg="0" m="0" s="0" A="0" K="0" mol="0" cd="0" rad="1" factor="1.0" offset="0.0"/>
        </Unit>
        <Unit name="m/s">
            <BaseUnit kg="0" m="1" s="-1" A="0" K="0" mol="0" cd="0" rad="0" factor="1.0" offset="0.0"/>
        </Unit>
        <Unit name="rad/s">
            <BaseUnit kg="0" m="0" s="-1" A="0" K="0" mol="0" cd="0" rad="1" factor="1.0" offset="0.0"/>
        </Unit>
        <Unit name="N">
            <BaseUnit kg="1" m="1" s="-2" A="0" K="0" mol="0" cd="0" rad="0" factor="1.0" offset="0.0"/>
        </Unit>
        <Unit name="N.m">
            <BaseUnit kg="1" m="2" s="-2" A="0" K="0" mol="0" cd="0" rad="0" factor="1.0" offset="0.0"/>
        </Unit>
    </UnitDefinitions>

    <VariableGroups>
		<Generic name="external_force">
            <Variable ref="external_force[1]" unit="N"/>
            <Variable ref="external_force[2]" unit="N"/>
            <Variable ref="external_force[3]" unit="N"/>
            <Variable ref="external_force[4]" unit="N.m"/>
            <Variable ref="external_force[5]" unit="N.m"/>
            <Variable ref="external_force[6]" unit="N.m"/>
        </Generic>
        
        <Generic name="current_velocity">
            <Variable ref="current_velocity[1]" unit="m/s"/>
            <Variable ref="current_velocity[2]" unit="m/s"/>
            <Variable ref="current_velocity[3]" unit="m/s"/>
			<Variable ref="current_velocity[4]" unit="rad/s"/>
            <Variable ref="current_velocity[5]" unit="rad/s"/>
            <Variable ref="current_velocity[6]" unit="rad/s"/>
        </Generic>
        
        <Generic name="position_6dof">
            <Variable ref="eta[1]" unit="m"/>
            <Variable ref="eta[2]" unit="m"/>
            <Variable ref="eta[3]" unit="m"/>
            <Variable ref="eta[4]" unit="rad"/>
            <Variable ref="eta[5]" unit="rad"/>
            <Variable ref="eta[6]" unit="rad"/>
        </Generic>
		
		<Generic name="position_3dof">
            <Variable ref="eta[1]" unit="m"/>
            <Variable ref="eta[2]" unit="m"/>
            <Variable ref="eta[6]" unit="rad"/>
        </Generic>

        <Generic name="velocity_body_6dof">
            <Variable ref="nu[1]" unit="m/s"/>
            <Variable ref="nu[2]" unit="m/s"/>
            <Variable ref="nu[3]" unit="m/s"/>
            <Variable ref="nu[4]" unit="rad/s"/>
            <Variable ref="nu[5]" unit="rad/s"/>
            <Variable ref="nu[6]" unit="rad/s"/>
        </Generic>
		
		<Generic name="velocity_body_3dof">
            <Variable ref="nu[1]" unit="m/s"/>
            <Variable ref="nu[2]" unit="m/s"/>
            <Variable ref="nu[6]" unit="rad/s"/>
        </Generic>

        <Generic name="velocity_ned_6dof">
            <Variable ref="eta_dot[1]" unit="m/s"/>
            <Variable ref="eta_dot[2]" unit="m/s"/>
            <Variable ref="eta_dot[3]" unit="m/s"/>
            <Variable ref="eta_dot[4]" unit="rad/s"/>
            <Variable ref="eta_dot[5]" unit="rad/s"/>
            <Variable ref="eta_dot[6]" unit="rad/s"/>
        </Generic>

        <LinearVelocity name="nmea_senti">
            <Variable ref="nu[1]" unit="m/s"/>
            <Variable ref="nu[2]" unit="m/s"/>
            <Variable ref="acc[1]" unit="m/s^2"/>
            <Variable ref="acc[2]" unit="m/s^2"/>
        </LinearVelocity>
		
		<Generic name="acceleration_6dof">
            <Variable ref="acc[1]" unit="m/s^2"/>
            <Variable ref="acc[2]" unit="m/s^2"/>
            <Variable ref="acc[3]" unit="m/s^2"/>
            <Variable ref="acc[4]" unit="rad/s^2"/>
            <Variable ref="acc[5]" unit="rad/s^2"/>
            <Variable ref="acc[6]" unit="rad/s^2"/>
        </Generic>
		
		<Generic name="vru_signal">
            <Variable ref="eta[4]" unit="rad"/>
            <Variable ref="eta[5]" unit="rad"/>
        </Generic>

		<Generic name="gyro_signal">
            <Variable ref="eta[6]" unit="rad"/>
            <Variable ref="nu[6]" unit="rad"/>
        </Generic>		
    </VariableGroups>
</OspModelDescription>